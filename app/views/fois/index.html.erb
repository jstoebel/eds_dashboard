<h1>Forms of Intention</h1>

<table id="forms_of_intention">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>B#</th>
    <th>Major</th>
    <th>Seeking Cert.</th>
  </tr>
<% @fois.each do |foi| %>
  <tr>
    <td><%= foi.student.FirstName %></td>
    <td><%= foi.student.LastName %></td>
    <td><%= foi.student.Bnum %></td>
    <td><%= foi.student.CurrentMajor1%></td>
    <td><%= foi.seek_cert %></td>
  </tr>
<% end %>
</table>
  <%= form_tag import_fois_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>