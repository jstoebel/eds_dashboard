<h1>Forms of Intention</h1>

<table id="forms_of_intention">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>B#</th>
    <th>Major</th>
    <th>Seeking Cert.</th>
    <th>EDS Only</th>
    <th>Date Completed</th>
  </tr>
<% if @fois.present? %>
  <% @fois.each do |foi| %>
    <tr>
      <td><%= foi.student.FirstName %></td>
      <td><%= foi.student.LastName %></td>
      <td><%= foi.student.Bnum %></td>
      <td><%= foi.major.andand.name%></td>
      <td><%= foi.seek_cert %></td>
      <td><%= foi.eds_only %></td>
      <td><%= foi.date_completing%></td>
    </tr>
  <% end %>
<% end %>
</table>
  <%= form_tag import_fois_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>
