<!-- edit.html.erb -->
<%= link_to student_praxis_results_path(@test.student.AltID), class: "btn btn-default back-link glyphicon glyphicon-arrow-left" do %>
  <i class=" link-text"> Back</i>
<% end %>

<div class="praxis new">

    <%= error_messages_for(@test) %>

    <%= form_for @test, url: praxis_result_path(@test.AltID) do |f| %>

      <table summary="Praxis form fields">

        <caption>Edit Praxis Registration</caption>
        <%= render(partial: 'form', locals: {f: f})  %>
      </table>

    <div class="form-buttons">
      <%= submit_tag("Edit Test") %>

    <% end %>
</div>