<%= link_to banner_term_adm_st_index_path(@term), class: "btn btn-default back-link glyphicon glyphicon-arrow-left" do %>
  <i class=" link-text"> Back</i>
<% end %>

<div class="adm_st show">


  <table class="vertical-table" summary="adm_st detail view">
  <caption> 
 	<div class="header1"> <%=[@first_name, @last_name].join(', ') +': ' + @term.PlainTerm%> </div>
	<div class="header2">Application to Student Teaching Details</div>

  </caption>

    <tr>
      <th>Admission Decision</th>
      <td><%= @app.STAdmitted ? "Admitted" : "Not admitted"%></td>
    </tr>

    <tr>
      <th>Notes</th>
      <td><%= @app.Notes%></td>
    </tr>

  </table>

</div>