<%= link_to adm_st_index_path, class: "btn btn-default back-link glyphicon glyphicon-arrow-left" do %>
  <i class=" link-text"> Back</i>
<% end %>


<div class="adm_st new">

  <%= form_for(:adm_st, :url => adm_st_index_path) do |f| %>

	<table summary="adm_st form fields">

	    <caption>
      <div class='header1'>New Application to Student Teaching</div>

      </caption>
	    <%= error_messages_for(@app) %>

    <tr>
      <th> <%= f.label(:student_id, "Name")%> </th>
      <td> <%= f.select(:student_id, @students.map {|s| [name_details(s, file_as=true), s.id]}, {:prompt => 'Select a name'}) %></td>
    </tr>

    <tr>
      <th> <%= f.label(:BannerTerm_BannerTerm, "Term")%> </th>
      <td> <%= f.select(:BannerTerm_BannerTerm, @terms.map {|t| [t.readable, t.id]}, {:prompt => 'Select a term to apply'}) %></td>
    </tr>

  	<tr>
	<th></th>
	<td>
    <div class="form-buttons">
      <%= submit_tag("Submit") %>


	</td>
	</tr>
    </div>	
	</table>



  <% end %>
</div>
