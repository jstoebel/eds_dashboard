<%= link_to clinical_assignments_path, class: "btn btn-default back-link glyphicon glyphicon-arrow-left" do %>
  <i class=" link-text"> Back</i>
<% end %>

<%= form_for(@assignment, url: clinical_assignments_path) do |f| %>

	<table summary="clinical_assignments form fields">

    <caption>
    	<span class="header1"> Add New Clinical Assignment</span>

    </caption>

	<%= render(partial: 'form', locals: {f: f}) %>
	<br>
	<tr>
		<td><%= submit_tag("Create Assignment") %></td>
	</tr>

<% end %>
